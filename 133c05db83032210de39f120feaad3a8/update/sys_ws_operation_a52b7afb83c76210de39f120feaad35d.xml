        <name>Create Attendee</name>
        <operation_script><![CDATA[(function process( /*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {
   var body = request.body.data;
    var gr = new GlideRecord('x_1790195_event_ma_attendee');
    gr.initialize();
    gr.setValue('x_first_name', body.first_name);
    gr.setValue('x_last_name', body.last_name);
    gr.setValue('x_email', body.email);
    gr.setValue('x_contact_number', body.contact_number);
    gr.setValue('x_event_id', body.event_id);
    gr.insert();

    response.setStatus(201);
    return {
        result: "Attendee registered successfully"
    };
})(request, response);]]></operation_script>
